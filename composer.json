{
    "name": "m2mtech/ux-navigation",
    "description": "Twig/UX extensions for common navigation widgets for symfony.",
    "type": "symfony-bundle",
    "keywords": [
        "dark mode",
        "extension",
        "language selector",
        "navigation",
        "symfony",
        "symfony-ux",
        "twig extension",
        "twig",
        "ux"
    ],
    "homepage": "https://github.com/m2mtech/ux-navigation",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Mandl",
            "email": "tech@m2m.at",
            "homepage": "https://cms.m2m.at",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/intl": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^8.5.22|^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^5.4|^6.0",
        "symfony/webpack-encore-bundle": "^1.14",
        "symplify/easy-coding-standard": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "M2MTech\\UxNavigation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M2MTech\\UxNavigation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs-fix": "vendor/bin/ecs check --fix",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit 1G"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
